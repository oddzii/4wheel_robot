<?xml version="1.0"?>
<launch>
    
    <param name="robot_description" command="$(find xacro)/xacro.py '$(find myrobot_description)/urdf/myrobot.urdf.xacro'"/>

    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
        <param name="use_gui" value="False"/>
    </node>

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    </node>

    <!--<node pkg="rviz" type="rviz" name="rviz" args="-d $(find myrobot_description)/rviz/config_file.rviz" >
    </node>-->

    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find myrobot_description)/rviz/myrobot.rviz" >
    </node>


</launch>
